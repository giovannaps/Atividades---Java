import java.util.Scanner;


public class Main {
    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        System.out.print("Digite um número inteiro: ");
        int tabuadaNumero = s.nextInt();

        int resultadoTabuada = 0;
        for (int i = 1; i < 11; i++) {
            resultadoTabuada = (tabuadaNumero * i);
            System.out.println("A tabuada de " + tabuadaNumero + " x " + i + " = " + " é igual a: " +  resultadoTabuada);

        }


    }

    }
